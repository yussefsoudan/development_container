{
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/usr/local/bin/black",
  "clangd.arguments": ["--resource-dir=/usr/lib/llvm-12/lib/clang/12.0.0"],
  "coc.preferences.jumpCommand": "tabe",
  "git.addGBlameToVirtualText": true,
  "clangd.fallbackFlags": [
    "-std=c++17",
    "-Weverything", // enable all diagnostics 
    "-isystem", // adds a directory to system include search path
    "/usr/include"
  ],
  "clangd.semanticHighlighting": true,
  "coc.github.enable": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.formatOnSaveFiletypes": ["cmake", "cpp", "json", "python"]
  "cmake.cmakePath": "/usr/bin/cmake",
  "http.proxyStrictSSL": false,
  "codeLens.enable": true,
  "yaml.format.enable": true,
  "yaml.completion": true,
  "yaml.hover": true,
  "yaml.validate": true,
  "explorer.git.enable": true,
  "explorer.icon.enableNerdfont": true,
}
